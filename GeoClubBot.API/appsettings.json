{
  "ConnectionStrings": {
    "PostgreSQL": "your;connection;string"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "TimestampFormat": "[yyyy-MM-dd HH:mm:ss] "
      }
    }
  },
  "AllowedHosts": "*",
  "Discord": {
    "BotToken": "YourDiscordBotTokenHere",
    "ServerId": 123456789,
    "WelcomeMessage": "Welcome {{User}} to {{Guild}}! Member #{{NumMember}}.",
    "WelcomeTextChannelId": 123456789
  },
  "GeoGuessr": {
    "NcfaToken": "YourGeoGuessrNcfaTokenHere",
    "ClubId": "your-club-id",
    "Club": {
      /* See https://www.quartz-scheduler.net/documentation/quartz-3.x/tutorial/crontriggers.html for syntax */
      "SyncSchedule": "0 0 * ? * * *"
    }
  },
  "ActivityChecker": {
    /* See https://www.quartz-scheduler.net/documentation/quartz-3.x/tutorial/crontriggers.html for syntax */
    "Schedule": "0 0 0 ? * MON *",
    "TextChannelId": 123456789,
    "MinXP": 80,
    "GracePeriodDays": 2,
    "MaxNumStrikes": 2,
    "HistoryKeepTimeSpan": "30.00:00:00",
    "StrikeDecayTimeSpan": "178.00:00:00"
  },
  "ActivityReward": {
    "TextChannelId": 123456789,
    "MvpRoleId": 123456789
  },
  "ClubLevelChecker": {
    /* See https://www.quartz-scheduler.net/documentation/quartz-3.x/tutorial/crontriggers.html for syntax */
    "Schedule": "0 0 * ? * * *",
    "LevelUpMessageChannelId": 123456789
  },
  "DailyChallenges": {
    /* See https://www.quartz-scheduler.net/documentation/quartz-3.x/tutorial/crontriggers.html for syntax */
    "Schedule": "0 0 17 ? * * *",
    "TextChannelId": 123456789,
    "ConfigurationFilePath": "../ChallengesConfig.json",
    "FirstRoleId": 123456789,
    "SecondRoleId": 123456789,
    "ThirdRoleId": 123456789
  },
  "GeoGuessrAccountLinking": {
    "AdminChannelId": 123456789,
    "HasLinkedRoleId": 123456789,
    "ClubMemberRoleId": 123456789
  },
  "MemberPrivateChannels": {
    "CategoryId": 123456789,
    "Description": "This is your private text channel!"
  },
  "SelfRoles": {
    "TextChannelId": 123456789,
    "Roles": [
      {
        "RoleId": 123456789,
        "RoleEmoji": ":first_place:",
        "RoleDescription": "A role description"
      }
    ]
  },
  "AI": {
    "Active": false
  },
  "SQL": {
    "Migrate": false
  }
}
